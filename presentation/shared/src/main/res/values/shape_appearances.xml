<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="RoundedCorners">
        <item name="cornerSizeTopLeft">@dimen/corner_radius</item>
        <item name="cornerSizeTopRight">@dimen/corner_radius</item>
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="strokeWidth">@dimen/divider</item>
    </style>

    <style name="RoundedImageView">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
        <item name="strokeWidth">@dimen/divider</item>
        <item name="strokeColor">?colorOnPrimary</item>
    </style>

</resources>